{
  "manifest_version": 3,
  "name": "Aiya - AI Overview Remover",
  "version": "1.1",
  "description": "Add the Aiya blocker to remove the AI overview on each page",
  "icons": {
    "16": "images/plant.png",
    "32": "images/plant.png",
    "48": "images/plant.png",
    "128": "images/plant.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://www.google.com/search*"
      ]
    }
  ]
}
